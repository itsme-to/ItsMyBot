{
  "name": "itsmybot",
  "version": "3.9.3",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "make:addon": "node ./build/core/tools/makeAddon.js",
    "tools:translate": "node --env-file=.env ./build/core/tools/translate/index.js",
    "build:clean": "rm -rf ./build",
    "build:itsmybot": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:copy": "cpy \"src/**/*.yml\" \"build/\" --parents",
    "build:success": "echo '[ItsMyBot] Build completed successfully!'",
    "build": "npm run build:clean && npm run build:itsmybot && npm run build:copy && npm run build:success",
    "start": "node ./build/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsme-to/ItsMyBot"
  },
  "author": "@itsme.to",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itsme-to/ItsMyBot/issues"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "cpy-cli": "^5.0.0",
    "inquirer": "^9.2.23",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.4.5",
    "reflect-metadata": "^0.2.2"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "discord.js": "^14.24.0",
    "filtrex": "^3.1.0",
    "glob": "^11.0.1",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.0",
    "mysql2": "^3.12.0",
    "openai": "^5.12.2",
    "selfsigned": "^2.4.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "yaml": "^2.7.0"
  }
}
